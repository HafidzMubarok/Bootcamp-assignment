<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        nav {
            background-color: #333;
            padding: 10px;
        }
        nav a {
            color: white;
            text-decoration: none;
            margin: 0 15px;
            padding: 8px 16px;
        }
        nav a:hover {
            background-color: #575757;
            border-radius: 4px;
        }
    </style>
</head>
<body>
    <nav>
        <a href="/">Home</a>
        <a href="/about">About</a>
        <a href="/contact">Contact</a>
    </nav>
    <h1>Contact</h1>

    <h2>Contact List</h2>
    <div>
        <% if (!contacts.length) { %>
            <span>There is no contact yet!</span>
        <% }else { %>
            <% contacts.forEach(contact => { %>
                <li style="list-style: none;">
                    <span>Name: <%= contact.name  %></span>
                    <br>
                    <span>Email: <%= contact.email  %></span>
                    <!-- <span>Name: <a href="/contact/<%= contact.name %>"><%= contact.name  %></a></span> -->
                    <!-- <br>
                    <span>Phone Number: <%= contact.mobile  %></span> -->
                </li>
                <br>
            <% }) %>
        <% } %>
    </div>
</body>
</html>